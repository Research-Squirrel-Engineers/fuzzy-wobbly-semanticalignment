@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix skosplus: <https://w3id.org/skos-plus/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

skos:closeMatch a owl:ObjectProperty ;
    rdfs:label "SKOS closeMatch (7-star level 6)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    skosplus:degreeOfConnection 0.9380796757830466 ;
    skosplus:medianPerceivedProbability 0.9 ;
    skosplus:perceptionBand "Highly Likely"@en ;
    skosplus:starLevel 6 .

skos:exactMatch a owl:ObjectProperty ;
    rdfs:label "SKOS exactMatch (7-star level 7)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    skosplus:degreeOfConnection 1.0 ;
    skosplus:medianPerceivedProbability 0.95 ;
    skosplus:perceptionBand "Almost Certainly"@en ;
    skosplus:starLevel 7 .

skosplus:degreeOfConnection a owl:AnnotationProperty ;
    rdfs:label "degree of connection (0â€“1)"@en ;
    rdfs:comment "Numeric degree of connection for 7-star mapping relations; computed via a normalised saturating exponential function."@en .

skosplus:medianPerceivedProbability a owl:AnnotationProperty ;
    rdfs:label "median perceived probability"@en ;
    rdfs:comment "Median perceived probability for a perceptions phrase (derived from zonination/perceptions)."@en .

skosplus:perceptionBand a owl:AnnotationProperty ;
    rdfs:label "perception band"@en ;
    dct:source "https://github.com/zonination/perceptions" ;
    rdfs:comment "Internal explanatory link to a perceptions phrase/band used to justify a 7-star level (annotation only; not part of the object-property hierarchy)."@en .

skosplus:relatedMatchModerate a owl:ObjectProperty ;
    rdfs:label "relatedMatch moderate (7-star level 3)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skosplus:degreeOfConnection 0.5627416864987679 ;
    skosplus:medianPerceivedProbability 0.6 ;
    skosplus:perceptionBand "Better Than Even"@en ;
    skosplus:starLevel 3 .

skosplus:relatedMatchStrong a owl:ObjectProperty ;
    rdfs:label "relatedMatch strong (7-star level 4)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skosplus:degreeOfConnection 0.7310585786300049 ;
    skosplus:medianPerceivedProbability 0.7 ;
    skosplus:perceptionBand "Probable"@en ;
    skosplus:starLevel 4 .

skosplus:relatedMatchVeryStrong a owl:ObjectProperty ;
    rdfs:label "relatedMatch very strong (7-star level 5)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skosplus:degreeOfConnection 0.8516629019405073 ;
    skosplus:medianPerceivedProbability 0.8 ;
    skosplus:perceptionBand "Very Good Chance"@en ;
    skosplus:starLevel 5 .

skosplus:relatedMatchVeryWeak a owl:ObjectProperty ;
    rdfs:label "relatedMatch very weak (7-star level 1)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skosplus:degreeOfConnection 0.0 ;
    skosplus:medianPerceivedProbability 0.25 ;
    skosplus:perceptionBand "We Doubt"@en ;
    skosplus:starLevel 1 .

skosplus:relatedMatchWeak a owl:ObjectProperty ;
    rdfs:label "relatedMatch weak (7-star level 2)"@en ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf skos:relatedMatch ;
    skosplus:degreeOfConnection 0.32783654048853833 ;
    skosplus:medianPerceivedProbability 0.5 ;
    skosplus:perceptionBand "About Even"@en ;
    skosplus:starLevel 2 .

skosplus:starLevel a owl:AnnotationProperty ;
    rdfs:label "7-star level"@en ;
    rdfs:comment "Discrete 7-star mapping level (1..7) used by the SKOS-Plus extension."@en .

skos:relatedMatch a owl:ObjectProperty ;
    rdfs:domain skos:Concept ;
    rdfs:range skos:Concept .

skos:Concept a owl:Class .

